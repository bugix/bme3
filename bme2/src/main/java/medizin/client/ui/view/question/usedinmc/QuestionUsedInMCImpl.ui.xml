<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client" xmlns:i="urn:import:medizin.client.ui.widget.widgetsnewcustomsuggestbox.test.client.ui.widget.suggest.impl" xmlns:loader="urn:import:medizin.client.ui.widget.process">
	<ui:style src="../../../styles.css">	
	</ui:style>
	
	
	<g:HTMLPanel  addStyleNames="{style.applicationLoadingPopupViewStyle}">
		<loader:ApplicationLoadingView ui:field="loadingPopup"  visible="false"></loader:ApplicationLoadingView>
		<g:ScrollPanel>
			<c:CellTable ui:field="table" addStyleNames="{style.standardTableIcon}"/>		
		</g:ScrollPanel>
		<div>
			<div class="{style.controls} {style.controlsleft}" >
				<c:SimplePager display="{table}" ui:field="pager" />
			</div>
		</div>			
	</g:HTMLPanel>
		
</ui:UiBinder> 