<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:medizin.client.ui.widget">
	<ui:style>
		.button {
		margin: 1em;
		float: right;
		background: none;

		cursor: pointer;
		}
				.panel {
		margin: 1.5em;
		padding: 1em;
		float: right;
		background: none;

		cursor: pointer;
		}
	</ui:style>
	<ui:with type="medizin.shared.i18n.BmeConstants" field="cc"></ui:with>
	<g:HTMLPanel>
		<div class="ui-state-highlight ui-corner-all {style.panel}">
		<span class="ui-icon ui-icon-info" style="float: left; margin-right: 0.3em;"></span>
		<g:Label text="{cc.minor}"></g:Label>
		<m:IconButton icon="disk" ui:field="saveChange">Frage als neue unterversion Speichern</m:IconButton>
		<!-- <g:Button addStyleNames="{style.button}" ui:field="saveChange">
								<span class="ui-icon ui-icon-disk" style="float: left; margin-right: 0.3em;"></span>
								
								Frage als neue unterversion Speichern</g:Button> -->
		</div>
		<div style="height:2em;" />
		<div class="ui-state-error ui-corner-all  {style.panel}"><span class="ui-icon ui-icon-alert" style="float: left; margin-right: 0.3em;"></span>
		<g:Label text="{cc.major}"></g:Label>
		<m:IconButton icon="disk" ui:field="saveNew">Frage als neue Version Kopieren</m:IconButton>
		<!-- <g:Button addStyleNames="{style.button}" ui:field="saveNew">
								<span class="ui-icon ui-icon-disk" style="float: left; margin-right: 0.3em;"></span>
								Frage als neue Version Kopieren</g:Button> -->
		</div>
		
	</g:HTMLPanel>
</ui:UiBinder> 