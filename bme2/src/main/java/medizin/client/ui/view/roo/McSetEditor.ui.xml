<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:my="urn:import:medizin.client.ui.widget">
    <ui:style type="medizin.client.ui.view.roo.McSetEditor.Style">
        .editorPanelVisible {
          margin: 2px;
    	  overflow: hidden;
    	  padding: 5px;
        }

        .viewPanelVisible {
        }

        .editorPanelHidden, .viewPanelHidden {
          display: none
        }
    </ui:style>
    <g:HTMLPanel>
        <g:HTMLPanel ui:field="viewPanel">
            <g:Label ui:field="viewLabel"/>
        </g:HTMLPanel>
        <g:HTMLPanel ui:field="editorPanel">
        	<g:ValueListBox ui:field="picker"/>
            <my:IconButton ui:field="add"></my:IconButton> <br/>
            <g:FlexTable ui:field="table"/>
        </g:HTMLPanel>
    </g:HTMLPanel>
</ui:UiBinder>
