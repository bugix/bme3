<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client" xmlns:i="urn:import:medizin.client.ui.widget.widgetsnewcustomsuggestbox.test.client.ui.widget.suggest.impl">
	<ui:style src="../../../styles.css">
	.topLbl
	{
		margin-top: 5px;
	}
	
	.bottomLbl
	{
		margin-top: 5px;
		margin-right: 47px;
		margin-bottom: 25px;
	}
	
	.bottomLbl2
	{
		margin-top: 5px;
		margin-right: 61px;
	}
	
	.btnStyle
	{
		width: 150px;
	}
	
	.divStyle
	{
		width: 300px;
		margin-left: 875px;	
	}
	</ui:style>
	<g:HTMLPanel>
	<g:HorizontalPanel>
			<g:HTMLPanel>
				<g:ScrollPanel>
					<c:CellTable ui:field="table" addStyleNames="{style.standardTableIcon}"/>		
				</g:ScrollPanel>
				<div>
					<div class="{style.divStyle} {style.controls} {style.controlsleft}" >
						<c:SimplePager display="{table}" ui:field="pager" />
					</div>
				</div>			
			</g:HTMLPanel>
		</g:HorizontalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 