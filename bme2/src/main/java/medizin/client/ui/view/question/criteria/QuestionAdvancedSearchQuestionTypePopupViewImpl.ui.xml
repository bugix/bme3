<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:medizin.client.ui.widget">
	<ui:style src="../../../styles.css">
		.width20 {
			width: 20px;
		}
	</ui:style>
	
	<g:HorizontalPanel ui:field="parentPanel" spacing="4" verticalAlignment="middle">
		<g:HTMLPanel>
			<table>
				<tr>
					<td>
						<g:ValueListBox ui:field="bindType"></g:ValueListBox>
					</td>
					
					<td>
						<g:ValueListBox ui:field="comparison"></g:ValueListBox>
					</td>
					
					<td>
						<g:ValueListBox ui:field="searchQuestionType" width="150px"></g:ValueListBox>
					</td>
					
					<td id="questionTypelistBoxTr">
						<g:ValueListBox ui:field="questionTypelistBox" width="150px"></g:ValueListBox>
					</td>
					
					<td id="questionTypeSearchValueTr">
						<g:TextBox ui:field="questionTypeSearchValue" width="145px"></g:TextBox>
					</td>
					
					<td>
						<m:IconButton ui:field="addQuestionTypeClicked" icon="plusthick"></m:IconButton>			
					</td>
					
					<td>
						<m:IconButton ui:field="closeButton" icon="closethick"></m:IconButton>
					</td>
				</tr>
			</table>
		</g:HTMLPanel>
	</g:HorizontalPanel>
</ui:UiBinder> 