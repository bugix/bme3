	<ui:style src="../styles.css">
		.panelHTML {
			border: 0px solid #ddf;
			height: 100%;
			width: 100%;
		}
		
		.fields {
			margin: 0.5em;
			padding-right: 1em;
		}
		
		.label {
			min-height: 25px;
			font-weight: bold;
		}
		
		.label2 {
			float: left;
		}
		
		.button {
			margin-left: 0.5em;
			float: right;
			background: none;
			cursor: pointer;
		}
		
		.header {
			padding: 3px;
			color: #4B4A4A;
			text-shadow: #ddf 1px 1px 0;
			margin-bottom: 0;
			background-color: #E6EDED;
			font-size: 1.2em;
			font-weight: bold;
		}
		
		.underline {
			border-bottom: 1px solid #6F7277;
		}
		
		.boundary {
			padding-left: 10px;
			padding-right: 10px;
		}
		
		.bar {
			padding: 3px;
			background-color: #E6EDED;
		}
		
		.details {
			border: 1px solid #E6EDED;
			width: 100%;
		}
		
		.trenner {
			border-bottom: 1px solid #E6EDED;
		}
		
		.padleft5 {
			padding-left: 5px;
		}
		
		.valueHeight {
			min-height: 25px;
		}
	</ui:style>
