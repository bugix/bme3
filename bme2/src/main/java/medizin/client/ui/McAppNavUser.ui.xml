<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="styles.css">
		.mainNavPanelContent a {
	text-decoration: none !important;
	display: block;
	padding: 2px 2px 2px 2px;
	
}
.paddingLeft28px
{
	padding-left:28px !important;
	background: pink;
}

.mainNavPanelContent a:hover {
	background-color: #e6e6e6;
}
	</ui:style>
	<g:HTMLPanel>
	<g:DisclosurePanel ui:field="systemOweviewPanel" width="100%" open="true" addStyleNames='{style.mainNavPanel}'>
		<g:header></g:header>
		<g:VerticalPanel addStyleNames='{style.mainNavPanelContent}' width="100%">
			<g:Anchor ui:field="systemOverview"></g:Anchor>
	
			<g:Anchor ui:field="acceptQuestion"></g:Anchor>
			<g:Anchor ui:field="acceptAnswer"></g:Anchor>
			<g:Anchor ui:field="acceptAssQuestion"></g:Anchor>

		</g:VerticalPanel>
	</g:DisclosurePanel>

	<g:DisclosurePanel ui:field="questionPanel" width="100%" open="true" addStyleNames='{style.mainNavPanel}'>
		<g:header></g:header>
		<g:VerticalPanel addStyleNames='{style.mainNavPanelContent}' width="100%">
			<g:Anchor ui:field="question"></g:Anchor>

		</g:VerticalPanel>
	</g:DisclosurePanel>
	<g:DisclosurePanel ui:field="assementPanel" width="100%" open="true" addStyleNames='{style.mainNavPanel}'>
		<g:header></g:header>
		<g:VerticalPanel addStyleNames='{style.mainNavPanelContent}' width="100%">

			<g:Anchor ui:field="asignAssQuestion"></g:Anchor>

		</g:VerticalPanel>
	</g:DisclosurePanel>
	<div ui:field="deletethis"  >
			<g:Anchor ui:field="acceptPerson"></g:Anchor>
						<g:Anchor ui:field="openDemand"></g:Anchor>
							<g:DisclosurePanel ui:field="managementPanel">
		<g:header>Benutzerverwaltung</g:header>
		<g:VerticalPanel addStyleNames='{style.mainNavPanelContent}' width="100%">
			<g:Anchor ui:field="user"></g:Anchor>
		</g:VerticalPanel>
	</g:DisclosurePanel>
				<g:Anchor ui:field="questionType"></g:Anchor>
			<g:Anchor ui:field="institution"></g:Anchor>
						<g:Anchor ui:field="assesment"></g:Anchor>
									<g:Anchor ui:field="bookAssesment"></g:Anchor>
			<g:Anchor ui:field="staticContent"></g:Anchor>
	</div>
	</g:HTMLPanel>
</ui:UiBinder> 