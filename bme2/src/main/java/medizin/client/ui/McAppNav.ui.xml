<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	 xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="styles.css">
	.mainNavPanelContent a {
	text-decoration: none !important;
	display: block;
	padding: 2px 2px 2px 2px;
	
}

.mainNavPanelContent a:hover {
	background-color: #e6e6e6;
}
		
	</ui:style>
	<g:HTMLPanel>
	<g:DisclosurePanel ui:field="overviewPanel" width="100%" open="true" addStyleNames='{style.mainNavPanel}' visible="false">
		<g:header></g:header>
		<g:VerticalPanel addStyleNames='{style.mainNavPanelContent}' width="100%">
			<g:Anchor ui:field="systemOverview"></g:Anchor>
		</g:VerticalPanel>
	</g:DisclosurePanel>
	<g:DisclosurePanel ui:field="assessmentQuestionsPanel" width="100%" open="true" addStyleNames='{style.mainNavPanel}' visible="false">
		<g:header></g:header>
		<g:VerticalPanel addStyleNames='{style.mainNavPanelContent}' width="100%">
			<g:Anchor ui:field="question"></g:Anchor>
			<g:Anchor ui:field="questionInAssessment"></g:Anchor>
			<g:Anchor ui:field="notActivatedQuestion"></g:Anchor>
			<g:Anchor ui:field="notActivatedAnswer"></g:Anchor>	
			<g:Anchor ui:field="deactivatedQuestion"></g:Anchor>
		</g:VerticalPanel>
	</g:DisclosurePanel>
	
		<g:DisclosurePanel ui:field="reviewProcessPanel" width="100%" open="true" addStyleNames='{style.mainNavPanel}' visible="false">
		<g:header></g:header>
		<g:VerticalPanel addStyleNames='{style.mainNavPanelContent}' width="100%">	
			<g:Anchor ui:field="acceptQuestion"></g:Anchor>
			<g:Anchor ui:field="acceptAnswer"></g:Anchor>
		</g:VerticalPanel>
	</g:DisclosurePanel>
	
	<g:DisclosurePanel ui:field="assesmentsPanel" width="100%" open="true" addStyleNames='{style.mainNavPanel}' visible="false">
		<g:header></g:header>
		<g:VerticalPanel addStyleNames='{style.mainNavPanelContent}' width="100%">
			<g:Anchor ui:field="assesment"></g:Anchor>
			<g:Anchor ui:field="asignAssQuestion"></g:Anchor>
			<g:Anchor ui:field="bookAssesment"></g:Anchor>
		</g:VerticalPanel>
	</g:DisclosurePanel>
	
	<g:DisclosurePanel ui:field="administrationPanel" width="100%" open="true" addStyleNames='{style.mainNavPanel}' visible="false">
		<g:header></g:header>
		<g:VerticalPanel addStyleNames='{style.mainNavPanelContent}' width="100%">		
			<g:Anchor ui:field="questionType"></g:Anchor>
			<g:Anchor ui:field="institution"></g:Anchor>
			<g:Anchor ui:field="user"></g:Anchor>
		</g:VerticalPanel>
	</g:DisclosurePanel>
	
	</g:HTMLPanel>
</ui:UiBinder> 