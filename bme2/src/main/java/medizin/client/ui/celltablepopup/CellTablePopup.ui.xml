<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:com.google.gwt.user.cellview.client"
	 xmlns:s="urn:import:javax.swing" 
	 xmlns:o="urn:import:org.apache.xpath.operations">

	<ui:style src="../styles.css" >
	.popupPanel {
		margin: 6px 6px;
	}
	
	
	
	.noOutline {
		outline-style: none;
		overflow: hidden;	
		
	}
	</ui:style>
		<!-- spec start -->
		<!-- <g:FocusPanel ui:field="filterPanelRoot" addStyleNames="{style.noOutline}"> -->
		
		<g:FocusPanel ui:field="filterPanelRoot" >
		<g:ScrollPanel width="100%"  ui:field="scrollPanel" >	
			<g:HTMLPanel >
				
				 <b:CellTable ui:field="cellTable" addStyleNames="{style.standardTable}"  />
				
	</g:HTMLPanel>
	</g:ScrollPanel>
	</g:FocusPanel>
	
	<!--  spec end-->
</ui:UiBinder> 